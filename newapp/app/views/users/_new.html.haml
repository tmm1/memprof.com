= form_for user do |f|  
  = f.error_messages
  = f.label :username
  = f.text_field :username
  = f.label :name
  = f.text_field :name
  = f.label :email
  = f.text_field :email
  = f.label :password
  = f.password_field :password
  = f.label :password_confirmation
  = f.password_field :password_confirmation
  = f.submit "Sign up"
